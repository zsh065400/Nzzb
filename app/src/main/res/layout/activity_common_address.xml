<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fl_common_address_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="zzbcar.cckj.com.nzzb.view.activity.itemactivity.CommonAddressActivity">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentTop="true"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true">


    <include layout="@layout/activity_title"  />



   <LinearLayout
       android:orientation="vertical"
       android:background="#f5f5f5"
       android:layout_weight="1"
       android:layout_width="match_parent"
       android:layout_height="0dp">

    <ImageView
        android:id="@+id/iv_add_Address_un"
       android:layout_marginTop="100dp"
        android:layout_gravity="center"
        android:src="@mipmap/un_address"
        android:layout_width="80dp"
        android:layout_height="80dp" />

       <TextView
           android:id="@+id/tv_add_newAddress_describe"
           android:layout_marginTop="40dp"
           android:layout_gravity="center"
           android:textSize="18sp"
           android:text="您还没有收车地址哦!"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content" />
   </LinearLayout>

</LinearLayout>

    <TextView
        android:padding="18dp"
        android:id="@+id/tv_commom_address_add"
        android:gravity="center"
        android:textColor="@color/Zzb_BG"
        android:textSize="16sp"
        android:text="+添加新地址"
        android:background="#ff4048"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</RelativeLayout>


</FrameLayout>